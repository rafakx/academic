<p id="notice"><%= notice %></p>

<h1>Listing Teacher Courses</h1>

<table>
  <thead>
    <tr>
      <th>Year</th>
      <th>Course</th>
      <th>Course type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @teacher_courses.each do |teacher_course| %>
      <tr>
        <td><%= teacher_course.year %></td>
        <td><%= teacher_course.course_id %></td>
        <td><%= teacher_course.course_type_id %></td>
        <td><%= link_to 'Show', teacher_course %></td>
        <td><%= link_to 'Edit', edit_teacher_course_path(teacher_course) %></td>
        <td><%= link_to 'Destroy', teacher_course, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Teacher course', new_teacher_course_path %>
